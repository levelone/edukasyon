<p id="notice"><%= notice %></p>

<p>
  <strong>Subject:</strong>
  <%= @klass.subject %>
</p>

<p>
  <strong>Teacher:</strong>
  <%= @klass.teacher.full_name %>
</p>

<p>
  <strong>Students:</strong>
  <ul>
    <% @klass.enrolled_students.each do |student| %>
      <li>
        <%= student.full_name %>
        <a href='#'>un-enroll</a>
      </li>
    <% end %>
  </ul>
</p>

<%= link_to 'Edit', edit_admin_klass_path(@klass) %> |
<%= link_to 'Back', admin_klasses_path %>
