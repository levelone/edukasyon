<p id="notice"><%= notice %></p>

<p>
  <strong>Course:</strong>
  <%= @klass.course.name %>
</p>

<p>
  <strong>By:</strong>
  <%= @klass.teacher.full_name %>
</p>

<p>
  <strong>Students:</strong>
  <ul>
    <% @klass.enrolled_students.each do |student| %>
      <li><%= "#{student.full_name} (#{student.email})" %></li>
    <% end %>
  </ul>
</p>

<%= link_to 'Enroll', '#' %> |
<%= link_to 'Review', '#' %> |
<%= link_to 'Back', :back %>
